.wrapper {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: #F5F5F5;
  z-index: 10;
}
.wrapper-white {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: white;
  z-index: 10;
}
.scroll-wrapper {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}
.scroll-wrapper-addition-title {
  position: fixed;
  top: 11.733333vw;
  bottom: 0;
  left: 0;
  right: 0;
}
.scroll-wrapper-head-and-bottom {
  position: fixed;
  top: 19.733333vw;
  bottom: 14.933vw;
  left: 0;
  right: 0;
}
.scroll-wrapper-page-title {
  height: calc(100vh - 11.733333vw);
}
.scroll-wrapper-page-index {
  height: calc(100vh - 11.733333vw - 3.7333vw - 3.7333vw - 7.5vw);
}
.scroll-wrapper-index-page {
  height: calc(100vh - 11.7333vw - 11.6666vw - 2.6666vw - 37.3333vw - 2.6666vw - 13.3333vw - 15vw);
}
.scroll-wrapper-service-page {
  height: calc(100vh - 11.7333vw - 2.6666vw - 37.3333vw - 2.6666vw - 13.3333vw - 15vw);
}
.scroll-wrapper-gate-index-page {
  height: calc(100vh - 11.7333vw - 2.6666vw - 37.3333vw - 2.6666vw - 13.3333vw - 15vw);
}
.scroll-wrapper-gate-service-page {
  height: calc(100vh - 11.7333vw - 2.6666vw - 37.3333vw - 2.6666vw - 13.3333vw - 15vw - 18.6666vw);
}
.scroll-wrapper-text-style {
  font-size: 4vw;
  color: rgba(51, 136, 255, 1);
}

