// 高德地图
.amap-custom-content-marker {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 5.87vw;
  height: 5.87vw;
  background: rgba(36, 179, 179, 1);
  border: 0.66vw solid rgba(255, 255, 255, 1);
  box-shadow: 0 1.33vw 2.67vw rgba(20, 54, 102, 0.1);
  border-radius: 50%;
  opacity: 1;
}
.amap-custom-content-marker img {
  width: 3.2vw;
  height: 3.2vw;
}
.amap-marker-label {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 100vw;
  height: 10vw;
  background-color: transparent;
  border-width: 0;
}
.amap-custom-content-marker-info {
  padding: 2.67vw 5.33vw;
  box-shadow: 0 1.33vw 2.67vw rgba(20, 54, 102, 0.2);
  border-radius: 4.8vw;
  border-width: 0;
  background-color: white;
  font-size: 3.2vw;
  color: #4D4D4D;
}


// 修改原来组件样式
.cube-dialog-main{
    border-radius: 2.6667vw;
    width: 84vw;
    .cube-dialog-title-def{
        font-size: 4.2667vw;
        color: #333;
        margin:0
        height: 13.3333vw;
        display: flex;
        justify-content: center;
        align-items: center;
    }
    .cube-dialog-content{
        min-height: 9.3333vw;
        margin: 2.6667vw auto 5.3333vw auto;
    }
    .cube-dialog-btns{
        &:after{
            height: 100%;
            top: 25%;
            border-color: #DADFE6 !important;
        }
        .cube-dialog-btn{
            height: 16vw;
            line-height: 16vw;
            padding: 0;
            &:before{
                content: none;
            }
        }
    }
}

