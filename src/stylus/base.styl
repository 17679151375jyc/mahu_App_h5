.base_text_justify-align_display { /** 文字设置宽度两端居中 **/
  text-align:justify;
  text-align-last:justify;
  display: -moz-inline-box;
  display: inline-block;
}

.base_text_details_normal_line-clamp-1 {
  display: -webkit-box;
  display: -moz-box;
  -webkit-line-clamp: 1;
  -moz-line-clamp: 1;
  -ms-line-clamp: 1;
  -o-line-clamp: 1;
  -webkit-box-orient: vertical;
  -moz-box-orient: vertical;
  -ms-box-orient: vertical;
  -o-box-orient: vertical;
  overflow: hidden;
  font-size: 3.2vw;
  color: #333333;
  line-height: 5.2vw !important;
  word-break: break-all;
  text-overflow: ellipsis;
  white-space: nowrap;
}
